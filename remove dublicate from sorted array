package firstproject;
import java.util.*;
import java.lang.*;
import java.io.*;

public class Test3 {
	public static void main(String[] args) {
		int []arr= {0, 2, 2, 3, 4,5,6,6,6,7};
		int i=0,j=i+1;
		while(j<arr.length) {
			if(arr[i]!=arr[j]) {
				i++;
				
			}
			arr[i]=arr[j];
			j++;
		}
		
		
		
		
		for(int a:arr) 
			System.out.println(a);

}
}
